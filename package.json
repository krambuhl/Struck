{
  "name": "struck",
  "version": "0.1.1",
  "author": "Evan Krambuhl <thekrambuhl@gmail.com>",
  "keywords": [
    "view",
    "extend",
    "model"
  ],
  "main": "dist/struck.js",
  "scripts": {
    "develop": "gulp develop",
    "compile": "gulp compile",
    "start": "npm update express && node server.js",
    "test": "gulp test",
    "ci-test": "./node_modules/karma/bin/karma start --single-run true && npm run ci-climate",
    "ci-climate": "mv coverage/*/lcov.info coverage && codeclimate < coverage/lcov.info"
  },
  "config": {
    "blanket": {
      "data-cover-flags": {
        "engineOnly": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:krambuhl/struct.git"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "0.0.3",
    "express": "^4.7.0",
    "groc": "^0.6.3",
    "gulp": "^3.8.6",
    "gulp-concat": "^2.3.4",
    "gulp-concat-sourcemap": "^1.3.1",
    "gulp-docco": "0.0.4",
    "gulp-filter": "^1.0.0",
    "gulp-mocha-phantomjs": "^0.3.0",
    "gulp-rename": "^1.2.0",
    "gulp-sourcemaps": "^1.1.0",
    "gulp-uglify": "^0.3.1",
    "karma": "^0.12.23",
    "karma-coverage": "^0.2.6",
    "karma-coveralls": "^0.1.4",
    "karma-firefox-launcher": "^0.1.3",
    "karma-mocha": "^0.1.9",
    "karma-phantomjs-launcher": "^0.1.4",
    "mocha": "^1.21.4"
  },
  "dependencies": {
    "jquery": "^2.1.1",
    "lodash": "^2.4.1"
  }
}
